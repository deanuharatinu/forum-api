{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allowAfterThis": true}],
      "import/no-extraneous-dependencies": "off",
